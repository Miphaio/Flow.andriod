        <!-- </intent-filter> -->
        <!-- </receiver> -->


        <!-- Required. For publish channel feature -->
        <!-- JPUSH_CHANNEL 是为了方便开发者统计APK分发渠道。 -->
        <!-- 例如: -->
        <!-- 发到 Google Play 的APK可以设置为 google-play; -->
        <!-- 发到其他市场的 APK 可以设置为 xxx-market。 -->
        <!-- 渠道统计报表位于控制台页面的 “统计”-“用户统计”-“渠道分布” 中 -->
        <meta-data
            android:name="JPUSH_CHANNEL"
            android:value="developer-default" />
        <!-- Required. AppKey copied from Portal -->
        <meta-data
            android:name="JPUSH_APPKEY"
            android:value="5b8497fcd134f3f89362ef06" />

        <activity android:name=".DetailActivity" />
        <activity android:name=".TaskNewActivity" />
        <activity android:name=".TaskDetailActivity" />
        <activity android:name=".SettingActivity" />
        <activity android:name=".TaskDetailSellerActivity" />
        <activity android:name=".TaskDetailDoneActivity" />
        <activity android:name=".CreateTaskActivity"></activity>
    </application>

</manifest>